{{< layout}}

{{$main}}
<div class="slider-container">
  <div class="slider">
    {{#images}}
    <div class="slide">
      <img src="{{url}}" alt="{{alt}}">
    </div>
    {{/images}}
  </div>
  <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
  <a class="next" onclick="plusSlides(1)">&#10095;</a>
</div>
{{/main}}


